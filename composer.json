{
    "name": "sonarsoftwareinc/customer_portal",
    "description": "Sonar Customer Portal",
    "keywords": [
        "framework",
        "laravel"
    ],
    "license": "MIT",
    "type": "project",
    "require": {
        "php": ">=7.4",
        "ext-json": "*",
        "ext-bcmath": "*",
        "laravel/framework": "^6.0",
        "predis/predis": "^1.0",
        "mariuzzo/laravel-js-localization": "dev-master",
        "laravel/tinker": "^1.0",
        "fideloper/proxy": "^4.0",
        "doctrine/dbal": "^2.10",
        "stripe/stripe-php": "^7",
        "gocardless/gocardless-pro": "^1.5",
        "laravelcollective/html": "^6.0",
        "paypal/rest-api-sdk-php": "^1.7",
        "proengsoft/laravel-jsvalidation": "^4.5",
        "inacho/php-credit-card-validator": "^1.0",
        "sonarsoftwareinc/customer_portal_framework": "dev-master",
        "spatie/laravel-csp": "^2.6",
        "gmostafa/php-graphql-client": "^1.13",
        "spatie/data-transfer-object": "^2.8",
        "seankndy/fluent-sonar-api": "^1.1"
    },
    "repositories": [
        {
            "type": "vcs",
            "url": "https://github.com/seankndy/customer_portal_framework"
        }
    ],
    "require-dev": {
        "fzaninotto/faker": "~1.4",
        "mockery/mockery": "0.9.*",
        "phpunit/phpunit": "^7.0",
        "filp/whoops": "~2.0"
    },
    "autoload": {
        "classmap": [
            "database"
        ],
        "psr-4": {
            "App\\": "app/"
        },
        "files": [
            "app/Helpers/functions.php"
        ]
    },
    "autoload-dev": {
        "psr-4": {
            "Tests\\": "tests/"
        }
    },
    "scripts": {
        "post-root-package-install": [
            "php -r \"copy('.env.example', '.env');\""
        ],
        "post-create-project-cmd": [
            "php artisan key:generate"
        ],
        "post-install-cmd": [
            "Illuminate\\Foundation\\ComposerScripts::postInstall"
        ],
        "post-update-cmd": [
            "Illuminate\\Foundation\\ComposerScripts::postUpdate"
        ],
        "post-autoload-dump": [
            "Illuminate\\Foundation\\ComposerScripts::postAutoloadDump",
            "@php artisan package:discover"
        ]
    },
    "config": {
        "preferred-install": "dist"
    },
    "prefer-stable": true,
    "minimum-stability": "dev"
}
